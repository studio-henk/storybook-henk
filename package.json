{
  "name": "storybook-henk",
  "version": "1.0.0",
  "description": "test",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-docs": "storybook dev --docs",
    "build-storybook-docs": "storybook build --docs",
    "build:js": "tsc -p tsconfig.scripts.json && for f in public/assets/*.js; do base=${f%.js}; base=${base%.min}; terser \"$f\" -c -m -o \"$base.min.js\"; done",
    "watch:js": "tsc -p tsconfig.scripts.json --watch",
    "build:wc": "vite build && cp -r dist/assets/* public/assets/",
    "check:browsers": "npx browserslist",
    "css:dev": "NODE_ENV=development node build-css.js",
    "css:prod": "NODE_ENV=production node build-css.js",
    "watch:css": "nodemon -e css -w src/assets --ignore public/assets --exec \"npm run css:dev && npm run css:prod\""
  },
  "author": "nh",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-a11y": "^8.1.0",
    "@storybook/addon-designs": "^8.0.2",
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-interactions": "^8.1.0",
    "@storybook/addon-links": "^8.1.0",
    "@storybook/blocks": "^8.1.0",
    "@storybook/html": "^8.1.0",
    "@storybook/html-vite": "^8.1.0",
    "@storybook/test": "^8.1.0",
    "@whitespace/storybook-addon-html": "^6.0.5",
    "autoprefixer": "^10.4.21",
    "browserslist-config-baseline": "^0.5.0",
    "chromatic": "^13.1.4",
    "cssnano": "^7.1.0",
    "fast-glob": "^3.3.3",
    "nodemon": "^3.1.10",
    "postcss-cli": "^11.0.1",
    "postcss-discard-comments": "^7.0.4",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.2.4",
    "prettier": "^3.2.5",
    "react-syntax-highlighter": "^15.5.0",
    "storybook": "^8.1.0",
    "storybook-design-token": "^3.3.0",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.18",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "liquidjs": "^10.21.1"
  },
  "browserslist": [
    "extends browserslist-config-baseline/2023"
  ]
}
